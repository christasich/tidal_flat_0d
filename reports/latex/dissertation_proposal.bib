@article{huqSeaLevelRiseBangladesh1995,
  title = {Sea-{{Level Rise}} and {{Bangladesh}}: {{A Preliminary Analysis}}},
  issn = {0749-0208},
  shorttitle = {Sea-{{Level Rise}} and {{Bangladesh}}},
  abstract = {[Bangladesh has 115 million people (1989 data) living in a low-lying deltaic area associated with three major rivers\textemdash{}the Ganges, Brahmaputra, and Meghna. This preliminary study examines the potential impacts of a one-meter relative rise in sea-level rise on the existing population and infrastructure. Assuming no responses, the following major impacts were identified. (1) Area Inundation: Over 25,000 km{$^2$} ({$>$}17.5\% of the existing land area could be inundated (lie beneath high tide). This includes 85 cities and towns, one major port (Mongla), over 800 km of roads and 28 km of railways. (2) Population Displacement: More than 13 million people ({$>$}11\% of the population) could be displaced by inundation. (3) Ecosystem Destruction: Sundarbans, the world's largest mangrove forest (5,770 km{$^2$}) is threatened with inundation and hence total destruction, including the world-famous Royal Bengal Tiger. (4) Agricultural Losses: Over 1.2 million ha of breadbasket land, producing 16\% of the country's rice, could be lost due to inundation. To provide minimum protection against a one-meter rise in sea level (protection against a 20-year flood), Bangladesh will have to upgrade 4,800 km of existing embankments and build 4,000 km of new embankments. This will cost over one billion U.S. dollars at 1989 prices. Natural sedimentation and the resulting vertical accretion it produces is not considered. Sea-level rise will also interact with river flow and cyclones to increase the already severe problems of flooding in Bangladesh. Therefore, sea-level rise, in specific, and global climate change in general, is of great concern to Bangladesh.]},
  journal = {Journal of Coastal Research},
  author = {Huq, Saleemul and Ali, Syed Iqbal and Rahman, A. Atiq},
  year = {1995},
  keywords = { No DOI found},
  pages = {44-53},
  note = {00057}
}

