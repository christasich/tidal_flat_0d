  doi = {10/bcz7d3},
  author = {Harvey, Charles F. and Swartz, Christopher H. and Badruzzaman, A. B. M. and {Keon-Blute}, Nicole and Yu, Winston and Ali, M. Ashraf and Jay, Jenny and Beckie, Roger and Niedan, Volker and Brabander, Daniel and Oates, Peter M. and Ashfaque, Khandaker N. and Islam, Shafiqul and Hemond, Harold F. and Ahmed, M. Feroze},
